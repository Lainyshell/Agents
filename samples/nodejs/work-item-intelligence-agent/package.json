{
  "name": "work-item-intelligence-agent",
  "version": "1.0.0",
  "private": true,
  "description": "Unified Work Item Intelligence Agent - Reads Azure DevOps, classifies work, and integrates with Teams and Copilot Studio",
  "author": "Microsoft",
  "license": "MIT",
  "main": "./dist/index.js",
  "scripts": {
    "prebuild": "npm ci",
    "build": "tsc --build",
    "prestart": "npm run build",
    "prestart:anon": "npm run build",
    "start:anon": "node ./dist/index.js",
    "start": "node --env-file .env ./dist/index.js",
    "test-tool": "agentsplayground",
    "test": "npm-run-all -p -r start:anon test-tool"
  },
  "dependencies": {
    "@microsoft/agents-hosting-express": "^1.0.0",
    "axios": "^1.7.9",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@microsoft/m365agentsplayground": "^0.2.16",
    "@types/node": "^22.15.18",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.8.3"
  },
  "keywords": [
    "azure-devops",
    "work-items",
    "teams",
    "copilot-studio",
    "intelligence",
    "classification"
  ]
}
